{
	"[json]": {
		"editor.quickSuggestions": {
			"comments": "on",
			"strings": true
		},
		"editor.suggest.insertMode": "replace",
	},

	"luau-lsp.completion.addParentheses": true,
	"luau-lsp.completion.addTabstopAfterParentheses": true,
	"luau-lsp.completion.autocompleteEnd": true,
	"luau-lsp.completion.enabled": true,
	"luau-lsp.completion.enableFragmentAutocomplete": true,
	"luau-lsp.completion.imports.enabled": true,
	"luau-lsp.completion.imports.ignoreGlobs": [
		"**/_Index/**",
		"**/*.test.{luau,lua,ts,tsx}",
		"**/*.spec.{luau,lua,ts,tsx}"
	],
	"luau-lsp.completion.imports.requireStyle": "auto",
	"luau-lsp.completion.imports.separateGroupsWithLine": true,
	"luau-lsp.completion.imports.stringRequires.enabled": false,
	"luau-lsp.completion.imports.suggestRequires": true,
	"luau-lsp.completion.imports.suggestServices": true,
	"luau-lsp.diagnostics.workspace": true,
	"luau-lsp.fflags.enableNewSolver": true,
	"luau-lsp.ignoreGlobs": ["**/_Index/**", "Vendor/Icon/Packages/**"],
	"luau-lsp.index.maxFiles": 2500,
	"luau-lsp.inlayHints.functionReturnTypes": true,
	"luau-lsp.inlayHints.hideHintsForErrorTypes": false,
	"luau-lsp.inlayHints.hideHintsForMatchingParameterNames": true,
	"luau-lsp.inlayHints.makeInsertable": true,
	"luau-lsp.inlayHints.parameterNames": "none",
	"luau-lsp.inlayHints.parameterTypes": true,
	"luau-lsp.inlayHints.typeHintMaxLength": 50,
	"luau-lsp.inlayHints.variableTypes": false,
	"luau-lsp.sourcemap.autogenerate": true,
	"luau-lsp.sourcemap.enabled": true,
	"luau-lsp.sourcemap.rojoProjectFile": "test-place.project.json",
	"luau-lsp.sourcemap.useVSCodeWatcher": true,
	"luau.trace.server": "off",
}